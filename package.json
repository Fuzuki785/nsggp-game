{
  "name": "nsggp-game",
  "productName": "nsggp-game",
  "version": "1.0.0",
  "description": "Simple Phaser platformer game, with JSON level description files",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-packager ./ --all --out=./out --arch=x64 --overwrite"
  },
  "keywords": [
    "Phaser",
    "Electron",
    "Game",
    "Platformer"
  ],
  "author": {
    "name": "Loïc Bréard",
    "email": "loic.breard@etud.univ-jfc.fr"
  },
  "license": "MIT",
  "repository": "https://github.com/Hades785/nsggp-game.git",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "nsggp-game"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "phaser": "^3.22.0",
    "electron-squirrel-startup": "^1.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "electron": "8.5.2",
    "electron-packager": "^14.2.1"
  }
}
